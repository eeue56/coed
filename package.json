{
    "name": "@eeue56/coed",
    "version": "0.4.0",
    "description": "Write HTML in pure TS",
    "main": "build/coed.js",
    "files": [
        "./build/coed.js",
        "./build/coed.d.ts",
        "./build/svg.js",
        "./build/svg.d.ts"
    ],
    "exports": {
        ".": {
            "types": "./build/coed.d.ts",
            "import": "./build/coed.js",
            "default": "./build/coed.js"
        },
        "./svg": {
            "types": "./build/svg.d.ts",
            "import": "./build/svg.js",
            "default": "./build/svg.js"
        }
    },
    "typings": "build/coed.d.ts",
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "watch": "tsc -w -p tsconfig.json",
        "format": "npx prettier --write .",
        "test": "npx @eeue56/bach",
        "docs": "npx @eeue56/geiriadur"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/eeue56/coed.git"
    },
    "keywords": [
        "typescript",
        "elm",
        "html",
        "hiraeth"
    ],
    "author": "eeue56",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/eeue56/coed/issues"
    },
    "homepage": "https://github.com/eeue56/coed#readme",
    "dependencies": {
        "@eeue56/ts-core": "^3.0.1"
    },
    "devDependencies": {
        "@types/node": "^25.3.2",
        "typescript": "^5.8.3",
        "@eeue56/bach": "^0.2.1",
        "@eeue56/geiriadur": "^0.0.3"
    }
}
